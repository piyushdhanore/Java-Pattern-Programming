public class Pattern_2 {

	public static void main(String[] args) {
		char ch = 'a';
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.print(ch + " ");
			}
			System.out.println();
			ch++;
		}
	}
}

/* output:
 
	a a a a a 
	b b b b b 
	c c c c c 
	d d d d d 
	e e e e e 	
 */
